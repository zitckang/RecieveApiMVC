@using System.Data
@model DataTable

@{
    ViewData["Title"] = "Data from Web API";
}

<table>
    <thread>
        <tr>
            <th>@Html.DisplayName("Id")</th>
            <th>@Html.DisplayName("Code")</th>
            <th>@Html.DisplayName("Name")</th>
            <th>@Html.DisplayName("Area")</th>
            <th>@Html.DisplayName("Lat")</th>
            <th>@Html.DisplayName("Long")</th>
            <th>@Html.DisplayName("Population")</th>
        </tr>
    </thread>
    <tbody>
        @foreach (DataRow row in Model.Rows)
        {
            <tr>
                <td>@row["Id"]</td>
                <td>@row["Code"]</td>
                <td>@row["Name"]</td>
                <td>@row["Area"]</td>
                <td>@row["Lat"]</td>
                <td>@row["Long"]</td>
                <td>@row["Population"]</td>
            </tr>
        }
    </tbody>
</table>
